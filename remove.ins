[Initial]
Message=Deinstalliere KeePass...
DefVar $ExitCode$

[Aktionen]
ShowBitmap "%ScriptPath%\logo.png" "KeePass"
Sub_Uninstall
Sub_HandleExitCode

[WinBatch_Uninstall]
del "C:\ProgramData\keepass"
del "%userprofile%\Desktop\KeePass"

[Sub_HandleExitCode]
; check return code
Set $ExitCode$ = GetLastExitCode
comment "GetLastExitCode: " + $ExitCode$
if not($ExitCode$ = "0")
  LogError "Fatal: setup returned exit code " + $ExitCode$
  IsFatalError
endif
